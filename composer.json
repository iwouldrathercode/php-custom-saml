{
    "name": "iwouldrathercode/php-custom-saml-connect",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) for multiple IdPs, based on OneLogin toolkit which is much more lightweight than simplesamlphp.",
    "keywords": ["laravel","saml", "saml2", "onelogin"],
    "homepage": "https://github.com/iwouldrathercode/php-custom-saml-connect",
    "license": "MIT",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Shankar",
            "email": "psgganesh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "illuminate/support": ">=5.0.0",
        "iwouldrathercode/php-one-login-custom-saml": "1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Iwouldrathercode\\Saml2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iwouldrathercode\\Saml2\\Saml2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
